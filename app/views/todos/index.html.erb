<h1>Todos</h1>

<ul>
    <% @todos.each do |todo| %>
        <li>
            <input type="checkbox">
                <%= link_to todo.task, todo %>
                <%= link_to "Edit", edit_todo_path(todo) %>
            </input>
        </li>
    <% end %>
</ul>

<%= link_to "Create Todo", new_todo_path(@todo) %>